{
  "name": "workspace-env-ci-testing-monorepo",
  "version": "0.0.1",
  "description": "the monorepo containing all the code for workspace-env",
  "repository": "https://github.com/TClark1011/workspace-env.git",
  "author": "Thomas Clark",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/config-lerna-scopes": "^19.0.0",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@eslint/js": "^9.2.0",
    "@types/eslint": "^8.56.10",
    "commitizen": "^4.3.0",
    "eslint": "^9.2.0",
    "globals": "^15.2.0",
    "inquirer": "9",
    "lerna": "^8.1.3",
    "lerna-changelog": "^2.2.0",
    "prettier": "3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "engines": {
    "node": "22.1.0"
  },
  "scripts": {
    "commit": "git-cz",
    "build-run-cli": "pnpm --filter workspace-env build:cli && mv ./packages/workspace-env/bin.js ./bin.js && node ./bin.js",
    "release": "lerna-changelog"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "changelog": {
    "labels": {
      "feature": "New Feature",
      "fix": "Bug Fixes"
    }
  }
}
