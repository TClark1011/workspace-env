{
  "name": "workspace-env",
  "version": "0.0.13",
  "description": "sync envs across your monorepo packages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TClark1011/workspace-env.git"
  },
  "author": "Thomas Clark",
  "license": "MIT",
  "private": false,
  "bin": {
    "workspace-env": "bin.js"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "vitest",
    "test:only": "node --require ts-node/register --require ./src/tests/mocks.ts --test --test-only \"**/*.test.ts\"",
