    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "cmd-ts": "^0.13.0",
    "glob": "^10.3.15",
    "yaml": "^2.4.2",
    "zod": "^3.23.8"
  }
}
