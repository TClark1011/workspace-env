{
  "name": "workspace-env",
  "version": "0.0.19",
  "description": "sync envs across your monorepo packages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TClark1011/workspace-env.git"
  },
  "author": "Thomas Clark",
  "license": "MIT",
  "private": false,
  "bin": {
    "workspace-env": "bin.js"
  },
  "main": "build/index.js",
  "scripts": {
    "start": "node bin.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "pnpm build:lib && pnpm build:cli",
    "build:lib": "tsup src/index.ts --dts --out-dir=build",
    "build:cli": "esbuild src/cli/bin.ts --bundle --platform=node --target=node10.4 --outfile=bin/index.js",
    "prepublish": "pnpm typecheck && pnpm lint && pnpm test && pnpm build",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "20.12.11",
    "esbuild": "^0.21.2",
    "memfs": "^4.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "cmd-ts": "^0.13.0",
    "glob": "^10.3.15",
    "yaml": "^2.4.2",
    "zod": "^3.23.8"
  }
}
